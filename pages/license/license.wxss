.background-con{
  background-color: rgb(245, 186, 10);
  position: fixed;
  height: 100%;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;/*垂直居中*/
}
.datatime-text{
  color: white;
  padding: 0.7rem;
}
.license-con{
  width: 92%;
  border-radius: 5px;
  background-color: white;
}
.head-con{
  border-radius: 5px 5px 0px 0px;
  background-color: rgb(172, 94, 28);
  text-align: center;
  padding: 1rem;
}
.head-font{
  font-size: 1.5rem;
  color: white;
  position: relative;
}
.img-con{
  padding-left: 25%;
  width: 50%;
  display: flex;
  text-align: center;
}
.img-icon{
  margin: auto;
  padding: auto;
}
.black-blod-md{
  color: black;
  font-size: 1.2rem;
  display: block;
  margin-bottom: 0.8rem;
  font-weight: 600;
}
.black-md{
  color: gray;
  font-size: 1.2rem;
  display: block;
  margin-bottom: 0.8rem;
}
col-md{
  width: 200px;
  display: flex;
  flex-direction: column;
}
.btn{
  background-color: rgb(245, 186, 10);
  padding-top: 1rem;
  padding-bottom: 1rem;
  border-radius: 3rem;
}